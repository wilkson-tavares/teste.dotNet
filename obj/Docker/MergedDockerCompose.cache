services:
  theos:
    build:
      context: C:\Projetos\Theos
      dockerfile: Theos/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
    image: theos
    ports:
    - 80/tcp
  theoswebapplication:
    build:
      context: C:\Projetos\Theos
      dockerfile: TheosWebApplication/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
    image: theoswebapplication
    ports:
    - 80/tcp
version: '3.0'